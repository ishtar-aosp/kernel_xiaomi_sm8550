# Display
CONFIG_XIAOMI_DISP_NOTIFIER=m

# Fingerprint
CONFIG_FINGERPRINT_GOODIX_FOD=m
CONFIG_INPUT_FINGERPRINT=y

# IR
CONFIG_IR_SPI=m

# MTD
CONFIG_MTD=m
CONFIG_MTD_BLOCK=m
CONFIG_MTD_BLOCK2MTD=m
CONFIG_MTD_OOPS=m

# Product
CONFIG_TARGET_PRODUCT_VERMEER=y

# Thermal
CONFIG_MI_THERMAL_INTERFACE=m
CONFIG_MI_THERMAL_MULTI_CHARGE=y

# Touchscreen
CONFIG_TOUCHSCREEN_GOODIX_BRL=m
CONFIG_TOUCHSCREEN_XIAOMI_TOUCHFEATURE=m

# Regulator
CONFIG_REGULATOR_WL2866D=m
CONFIG_REGULATOR_WL2868C=m

# Xiaomi HWID
CONFIG_MI_HARDWARE_ID=m

# CONFIG_QCOM_WCD_USBSS_I2C is not set
